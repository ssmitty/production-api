# Example secrets environment variables template
# Copy this file to .env.secrets and fill in your actual values
# NEVER commit .env.secrets to version control

# Database Configuration (Required)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Alpha Vantage API Configuration (Required)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# OpenAI API Configuration (Optional - for fallback matching)
OPENAI_API_KEY=sk-your_openai_api_key_here

# Server Configuration (Optional)
PORT=8080
HOST=0.0.0.0
DEBUG=true
